{
  "name": "svgeditor",
  "displayName": "SVG Editor",
  "description": "Visual and literal SVG editor for VSCode.",
  "version": "0.3.1",
  "publisher": "henoc",
  "icon": "images/SVG_logo.png",
  "galleryBanner": {
    "color": "#5c2d91",
    "theme": "dark"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Henoc/svgeditor/issues",
    "email": "agekutar@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Henoc/svgeditor"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^0.10.7"
  },
  "activationEvents": [
    "onCommand:extension.openSvgEditor",
    "onCommand:extension.newSvgEditor"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.openSvgEditor",
        "title": "Open SVG Editor"
      },
      {
        "command": "extension.newSvgEditor",
        "title": "New File with SVG Editor"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "extension.openSvgEditor",
          "when": "resourceLangId == svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run elm",
    "elm": "elm make src/Main.elm --output resources/main.js",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && ava out/test --verbose"
  },
  "devDependencies": {
    "@types/node": "*",
    "tslint": "^5.8.0",
    "typescript": "^2.6.1",
    "vscode": "^1.0.0"
  },
  "dependencies": {
    "@types/ejs": "^2.3.33",
    "ejs": "^2.5.7",
    "html": "^1.0.0"
  }
}
