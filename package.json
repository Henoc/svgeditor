{
  "name": "svgeditor",
  "displayName": "SVG Editor",
  "description": "Visual and literal SVG editor for VSCode.",
  "version": "1.0.0",
  "publisher": "henoc",
  "icon": "images/SVG_logo.png",
  "galleryBanner": {
    "color": "#FFCB77",
    "theme": "light"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Henoc/svgeditor/issues",
    "email": "agekutar@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Henoc/svgeditor"
  },
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.25.0"
  },
  "activationEvents": [
    "onCommand:svgeditor.openSvgEditor",
    "onCommand:svgeditor.newSvgEditor"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "svgeditor.openSvgEditor",
        "title": "Open SVG Editor"
      },
      {
        "command": "svgeditor.newSvgEditor",
        "title": "New File with SVG Editor"
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "svgeditor.openSvgEditor",
          "when": "resourceLangId == svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./ && npm run browserify",
    "browserify": "browserify out/main.js -o resources/bundle.js",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "@types/ejs": "^2.5.0",
    "@types/jsdom": "^11.0.6",
    "@types/node": "^9.4.0",
    "@types/uuid": "^3.4.3",
    "@types/xmldoc": "^1.1.3",
    "browserify": "^16.2.2",
    "tslint": "^5.9.1",
    "typescript": "^2.6.2",
    "vscode": "^1.1.18"
  },
  "dependencies": {
    "ejs": "^2.5.7",
    "jsdom": "^11.12.0",
    "uuid": "^3.3.2",
    "vue": "^2.5.16",
    "xmldoc": "^1.1.2"
  }
}
